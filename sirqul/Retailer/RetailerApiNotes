        -d "version=3.15" \
        -d "accountId=2547109" \
        -d "retailerId=199012" \
        -d "name=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX" \
        https://dev-test.sirqul.com/api/3.15/retailer/update

